// set the size of the canvas
size(400, 200);
// paint the background purple
background(208, 170, 208);
// set line color and width and fill color
stroke(246, 232, 113);
strokeWeight(20);
fill(113, 70, 132);
// draw the ellipse in the center of the canvas
ellipse(width/2, height/2, width/2, height/2);
